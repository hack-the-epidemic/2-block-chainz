{
  "name": "@linniaprotocol/linnia-smart-contracts",
  "version": "0.1.3",
  "description": "Linnia Smart Contracts",
  "main": "truffle-config.js",
  "files": [
    "build",
    "contracts"
  ],
  "scripts": {
    "start": "npm run start:testrpc",
    "start:testrpc": "ganache-cli -p 7545 -i 5777",
    "migrate": "truffle migrate",
